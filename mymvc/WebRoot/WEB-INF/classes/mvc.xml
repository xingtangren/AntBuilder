<?xml version="1.0" encoding="UTF-8"?>
<mvc xmlns="http://www.mymvc.org" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.mymvc.org mvc.xsd ">
	<actions>
		<action name="login" class="cn.com.zuo.action.LoginAction">
			<result name="success">WEB-INF/pages/index.jsp</result>
			<result name="error">WEB-INF/pages/error.jsp</result>
		</action>
		
		<action name="hello" class="cn.com.zuo.action.LoginAction" method="hello">
			<result name="hello">WEB-INF/pages/index.jsp</result>
			<result name="error">WEB-INF/pages/error.jsp</result>
		</action>
		
	</actions>
</mvc>
